{
    "name": "monitant",
    "version": "1.0.0",
    "description": "",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aramics/monitant.git"
    },
    "keywords": [
        "web3",
        "erc20",
        "balance",
        "monitor",
        "react",
        "vite",
        "typescript",
        "template",
        "ghactions",
        "vitest"
    ],
    "author": "aramics",
    "license": "MIT",
    "homepage": "https://github.com/aramics/monitant",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:ci": "tsc && vite build && cp 404.html dist/",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ci": "vitest run --coverage",
        "serve": "vite preview",
        "prepare": "husky install",
        "lint": "eslint --cache --fix ./src",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|tsx|css|json)\"",
        "precommit": "pnpm format && pnpm lint",
        "prepush": "pnpm format && pnpm lint && pnpm test:ci && pnpm build"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@commitlint/prompt-cli": "^17.0.3",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.3.0",
        "@types/node": "^18.6.4",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "@vitejs/plugin-react": "^2.0.0",
        "c8": "^7.12.0",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-testing-library": "^5.6.0",
        "happy-dom": "^6.0.4",
        "husky": "^8.0.1",
        "lint-staged": "^11.2.3",
        "prettier": "^2.7.1",
        "typescript": "^4.6.4",
        "vite": "^3.0.0",
        "vite-tsconfig-paths": "^3.5.0",
        "vitest": "^0.21.0"
    }
}
